-- To run the script again on the same save run in the console the following command
-- Game.GetQuestsSystem():SetFact("mod_all_wardrobe_unlocked", 0)
-- and then reload mods or save and reload game.

local function makeItemFullName (itemName)
    return "Items."..itemName
end

local function unlockWardrobe (player)
    local questSystem = Game.GetQuestsSystem()

    if questSystem:GetFact(MOD_FACT_NAME) == 1 then
        return
    end

    local transactionSystem = Game.GetTransactionSystem()

    for _, itemName in ipairs(clothingItemsNames) do
        local tid = TweakDBID.new(makeItemFullName(itemName))
        local itemid = ItemID.new(tid)

        local result = transactionSystem:GiveItem(player, itemid, 1)
        if not result then
            print("WARNING: Could not add item "..itemName)
            goto continue
        end

        result = transactionSystem:RemoveItem(player, itemid, 1)
        if not result then
            print("WARNING: Could not remove item "..itemName)
        end

        ::continue::
    end

    questSystem:SetFact(MOD_FACT_NAME, 1)
end

function Mod:new ()
    registerForEvent("onInit", function ()
        Override('PlayerPuppet', 'OnMakePlayerVisibleAfterSpawn', function (this, evt, wrapped)
            wrapped(evt)
            unlockWardrobe(this)
        end)
    end)
end

return Mod:new()
